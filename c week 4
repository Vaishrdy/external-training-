/* 
//Private
class A{
    private int data=10;
    public void msg(){
        System.out.println ("Helloworld");
    }
}
public class week4{
    public static void main(String args[]){
        A obj=new A();//OK:Class A is Public
        System.out.println(obj.data);//Compile time error : data is Private
        obj.msg();//OK:msg is public
    }
}*/
